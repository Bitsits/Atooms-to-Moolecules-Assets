         <script type="text/javascript"><!--
          $(function(){
           $('#header-container ol').jcarousel({
            vertical:false,
            wrap:'circular',
            scroll:1,
            itemFallbackDimension:983,
            auto:10
           });
           $('#page-links a').click(function(e){
            e.preventDefault();
            $(this).css('font-weight', 'bold');
            $(this).siblings().css('font-weight', 'normal');
            $('#header-container ol').data('jcarousel').scroll($(this).parent().children('a').index(this), true);
           });
           $('#page-arrow-link1 a').click(function(e){
            e.preventDefault();
            $('#header-container ol').data('jcarousel').prev();
           });
           $('#page-arrow-link2 a').click(function(e){
            e.preventDefault();
            $('#header-container ol').data('jcarousel').next();
           });
           $('#recent-forum-posts').jcarousel({
            vertical:true,
            wrap:'circular',
            scroll:1
           });
           $('#recent-forum-posts .vert-scroll-down').click(function(e){
            e.preventDefault();
            $('#recent-forum-posts').data('jcarousel').next();
           });
           $('#recent-blog-posts').jcarousel({
            vertical:true,
            wrap:'both',
            scroll:1
           });
           $('#recent-blog-posts .vert-scroll-down').click(function(e){
            e.preventDefault();
            $('#recent-blog-posts').data('jcarousel').next();
           });
           $('#accordion').accordion({
            active:2,
            collapsible:false,
            autoHeight:false,
            clearStyle:true,
            animated:false
           });
           $('#accordion').accordion('activate', 0);
           $('#apps-carousel').jcarousel({
