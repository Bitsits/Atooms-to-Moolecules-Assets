          $(document).ready(function() {
           $('#new-user-list').jcarousel({
            vertical:true,
            wrap:'circular',
            scroll:1,
            itemFallbackDimension:110
           });
           $('#new-user-list .vert-scroll-down').click(function(e){
            e.preventDefault();
            $('#new-user-list').data('jcarousel').next();
           });
           $('#top-user-list').jcarousel({
            vertical:true,
            wrap:'circular',
            scroll:1,
            itemFallbackDimension:110
           });
           $('#top-user-list .vert-scroll-down').click(function(e){
            e.preventDefault();
            $('#top-user-list').data('jcarousel').next();
           });
           $('.tab-link').click(moveTabs);
           $(function() {
            $("#tabs").tabs();
            $('#tabs ul').children().eq(0).find('.tabs-mid').css('color', '#666');
            $('#tabs').find('.tabs-left').first().css('background-image', 'url(/media/home/css/ui-lightness/images/tabs_left_first.png)');
            $("#tabs").bind("tabsselect", function(event, ui) {
             var selected = ui.index;
             $('#tabs ul').children().find('.tabs-mid').css('color', '#fff');
             $('#tabs ul').children().eq(selected).find('.tabs-mid').css('color', '#666');
             if(selected != 0){
              $('#tabs').find('.tabs-left').first().css('background-position', '0px -39px');
              } else {
              $('#tabs').find('.tabs-left').first().css('background-position', '0 0');
             }
            });
           });
          });
          function moveTabs(next) {
           $('.tab-link').parent().removeClass('tab-nav-onstate');
           $(this).parent().addClass('tab-nav-onstate');
           $(this).children().removeClass('tab-nav-onstate');
           $(this).children().css('color', '#fff');
           if($(this).parent().index() == 0){
            $('.tab-content-right').css('display', 'none');
            $('.tab-content-left').css('display', 'block');
           } else {
            $('.tab-content-left').css('display', 'none');
            $('.tab-content-right').css('display', 'block');
           }
          }
          // -->
         </script>
