             $(listItem).removeClass('first-item');
            }}
           });
           $('#apps-carousel .carousel-ui-left').click(function(e){
            e.preventDefault();
            $('#apps-carousel').data('jcarousel').prev();
           });
           $('#apps-carousel .carousel-ui-right').click(function(e){
            e.preventDefault();
            $('#apps-carousel').data('jcarousel').next();
           });
           $('#latest-content-carousel').jcarousel({
            vertical:false,
            wrap:'circular',
