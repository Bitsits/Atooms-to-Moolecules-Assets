            itemFallbackDimension:208,
            itemFirstInCallback: {onBeforeAnimation:function(carouselInstance,listItem,index,triggeringEvent) {
             $(listItem).addClass('first-item');
            }},
            itemFirstOutCallback: {onBeforeAnimation:function(carouselInstance,listItem,index,triggeringEvent) {
             $(listItem).removeClass('first-item');
            }}
           });
           $('#video-carousel .carousel-ui-left').click(function(e){
            e.preventDefault();
            $('#video-carousel').data('jcarousel').prev();
           });
           $('#video-carousel .carousel-ui-right').click(function(e){
            e.preventDefault();
            $('#video-carousel').data('jcarousel').next();
           });
           setTimeout(adjustWidths, 1000);
           accordionDefault();
          });
          function adjustWidths() {
           $('#latest-content-carousel .carousel').css('width', '2388px');
           $('#apps-carousel .carousel').css('width', '2388px');
           $('#video-carousel .carousel').css('width', '2388px');
          }
          function accordionDefault() {
           // IMPORTANT - this is what sets the default accordion state:
           // 0 - latest content
           // 1 - videos
           // 2 - app spotlight
           $('#accordion').accordion('activate', 0);
           $('#accordion').accordion('option', 'animated', 'slide');   // back to animated open/close on accordion
          }
          // -->
         </script>
